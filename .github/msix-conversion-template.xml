<?xml version="1.0" encoding="utf-8"?>
<MsixPackagingToolTemplate xmlns="http://schemas.microsoft.com/appx/msixpackagingtool/template/2018">
  <Settings 
    AllowTelemetry="false" 
    ApplyAllPrepareComputerFixes="false" 
    GenerateCommandLineFile="true" 
    AllowPromptForPassword="false"
    EnforceMicrosoftStoreRequirements="false"
    ServerPortNumber="1599"
    AddPackageIntegrity="false"
    xmlns:p7="http://schemas.microsoft.com/msix/msixpackagingtool/template/2004"
    xmlns:p6="http://schemas.microsoft.com/msix/msixpackagingtool/template/2001"
    xmlns:p5="http://schemas.microsoft.com/msix/msixpackagingtool/template/1904"
    xmlns:p4="http://schemas.microsoft.com/msix/msixpackagingtool/template/2007" />
  <PrepareComputer 
    DisableWindowsSearchService="true"
    DisableWindowsUpdateService="true" />
  <SaveLocation 
    PackagePath="[MSIXOutputPath]" />
  <Installer InstallLocation="" />
  <PackageInformation 
    PackageName="MarcoGomiero.FeedFlowRSSReader" 
    PackageDisplayName="FeedFlow - RSS Reader" 
    PublisherName="Marco Gomiero" 
    PublisherDisplayName="[PublisherDisplayName]" 
    Version="[AppVersion]" 
    PackageDescription="FeedFlow">
    <Resources>
      <Resource Language="en-us" />
      <Resource Language="de" />
      <Resource Language="es" />
      <Resource Language="et" />
      <Resource Language="fr" />
      <Resource Language="gl" />
      <Resource Language="he" />
      <Resource Language="it" />
      <Resource Language="pl" />
      <Resource Language="pt-br" />
      <Resource Language="ru" />
      <Resource Language="sk" />
      <Resource Language="ta" />
      <Resource Language="uk" />
      <Resource Language="vi" />
      <Resource Language="zh-cn" />
    </Resources>   
    <Capabilities>
      <Capability Name="runFullTrust" />
      <Capability Name="internetClient" />
    </Capabilities>
  </PackageInformation>
</MsixPackagingToolTemplate>